grp_import <- function(path=path) {
  
  files<-list.files(path = path,pattern = ".grp")
  
  columns = c("Gene","Signature") 
  tmp = data.frame(matrix(nrow = 1, ncol = length(columns))) 
  colnames(tmp) = columns
  
  for (i in files) {
    
    x<-read.table(paste0(path,i))
    x2<-x[-1,]
    Signature<-data.frame(Gene=x2,Signature=rep(x[1,],n=length(x2)))
    tmp<-na.omit(rbind(tmp,Signature))
    
  }
}
